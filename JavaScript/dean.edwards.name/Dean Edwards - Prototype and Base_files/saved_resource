<pre class="javascript">
var Class1 = Base.extend({

    constructor: function()
    {
        this.inherit = true;
    },

    inherit: null,

    method: function()
    {
        alert(this.inherit);
    }
});

var Class2 = Class1.extend({

    method2: function()
    {
        alert(this.inherit);
    }
});

var class2 = new Class2();
class2.method(); // shows "true"
class2.method2(); // shows "undefined"
</pre>
